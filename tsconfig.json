    "jsx": "react-jsx",
    "module": "commonjs",
    "outDir": "./dist",
    "paths": {
      "~/*": ["./app/src/*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "es2016"
  }
}
